    nasshare -a add -p 1 -s "name=sd, capacity=10gb"
    nasshare -a mod -i 1 -s "name=sd2, capacity=20gb"
    nasshare -a del -i 1
    nasshare -a mount -i 1
    nasshare -a unmount -i 1
